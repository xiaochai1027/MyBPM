  <process id="reservedCashRefund" name="站点资金管理-备用金降级" isExecutable="true">
    <startEvent id="create" name="会计发起备用金额降级" activiti:initiator="createdByCode"></startEvent>
    <userTask id="stationMasterConfirm" name="站长确认" activiti:assignee="${businessAreaFundHandler.getStationmaster(businessAreaCode)}"></userTask>
    <userTask id="cashierConfirm" name="出纳确认" activiti:candidateGroups="CASHIER"></userTask>
    <sequenceFlow id="flow3" sourceRef="stationMasterConfirm" targetRef="cashierConfirm"></sequenceFlow>
    <exclusiveGateway id="cashierConfirmResult" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="cashierConfirm" targetRef="cashierConfirmResult"></sequenceFlow>
    <serviceTask id="onProcessPass" name="确认通过" activiti:expression="${resultManager.onProcessPass(execution)}"></serviceTask>
    <sequenceFlow id="flow5" sourceRef="cashierConfirmResult" targetRef="onProcessPass">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${cashierConfirmed}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="operationManagementViceDirectorApproveResult" name="运营管理副总监审批结果"></exclusiveGateway>
    <sequenceFlow id="flow7" sourceRef="operationManagementViceDirectorApproveResult" targetRef="stationMasterConfirm">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${operationManagementViceDirectorApproved}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="modifyApplicationReport" name="修改申请报表" activiti:assignee="${createdByCode}"></userTask>
    <sequenceFlow id="flow8" sourceRef="operationManagementViceDirectorApproveResult" targetRef="modifyApplicationReport">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!operationManagementViceDirectorApproved}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="approveReapplyResult" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow9" sourceRef="modifyApplicationReport" targetRef="approveReapplyResult"></sequenceFlow>
    <serviceTask id="onProcessNotPass" name="放弃修改申请" activiti:expression="${resultManager.onProcessNotPass(execution)}"></serviceTask>
    <sequenceFlow id="flow10" sourceRef="approveReapplyResult" targetRef="onProcessNotPass">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!approveReapply}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow11" sourceRef="onProcessNotPass" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="cashierConfirmResult" targetRef="stationMasterConfirm">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!cashierConfirmed}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="operationManagementViceDirectorApprove" name="运营管理副总监审批" activiti:assignee="${employeeHandler.getSuperiorWithLocationCode(locationCode, 'OPERATION_MANAGEMENT_VICE_DIRECTOR', true)}"></userTask>
    <sequenceFlow id="flow21" sourceRef="operationManagementViceDirectorApprove" targetRef="operationManagementViceDirectorApproveResult"></sequenceFlow>
    <sequenceFlow id="flow29" sourceRef="cashierConfirmResult" targetRef="onProcessNotPass">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!cashierConfirmed}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow30" sourceRef="approveReapplyResult" targetRef="operationManagementViceDirectorApprove"></sequenceFlow>
    <sequenceFlow id="flow31" sourceRef="create" targetRef="operationManagementViceDirectorApprove"></sequenceFlow>
  </process>
